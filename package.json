{
    "name": "slick-lang",
    "displayName": "Slick Lang",
    "description": "Language support for Slick",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "slick",
            "aliases": ["Slick", "slick"],
            "extensions": [".slk"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "slick",
            "scopeName": "source.slk",
            "path": "./syntaxes/slick.tmLanguage.json"
        }]
    }
}